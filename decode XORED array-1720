class Solution {
    public int[] decode(int[] encoded, int first) {
        int k=encoded.length+1;
        int [] array=new int[k];
        array[0]=first;
     
        for(int i=0;i<encoded.length;i++)
        {
            array [i+1]=encoded[i] ^ array[i];
        }
        return array;
    }
}
